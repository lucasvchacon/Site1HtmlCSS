function change_month(select) {
    var day = $('#day');
    var val = $(day).val();
    $(day).empty();
    var option = '<option value="0" >Dia</option>';
    var month = parseInt( $(select).val() );
    for (var i=1;i <= Days[ month ];i++){ //add option days
        i = i < 10 ? "0"+i : i;
        option += '<option value="'+ i + '">' + i + '</option>';
    }
    $(day).append(option);
    if( val > Days[ month ] )
    {
        val = 1;
    }
    $(day).val(val);
}

